			<DependentUpon>AboutBoxForm.cs</DependentUpon>
		</Compile>
		<Compile Update="Forms\ExceptionForm.cs">
			<SubType>Form</SubType>
		</Compile>
		<Compile Update="Forms\ExceptionForm.Designer.cs">
