		</Compile>
		<Compile Update="Forms\OptionsForm.cs">
			<SubType>Form</SubType>
		</Compile>
		<Compile Update="Forms\OptionsForm.Designer.cs">
			<DependentUpon>OptionsForm.cs</DependentUpon>
		</Compile>
		<Compile Update="Forms\PathSettingsForm.cs">
